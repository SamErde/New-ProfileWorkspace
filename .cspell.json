{
  "version": "0.2",
  "language": "en",
  "import": [
    "@cspell/dict-powershell/cspell-ext.json",
    "@cspell/dict-csharp/cspell-ext.json"
  ],
  "words": ["MSIX", "RSAT"],
  "ignoreWords": [
    "sarif",
    "psscriptanalyzer",
    "POWERSHELLGALLERY",
    "stefanzweifel",
    "pozil",
    "antonyurchenko",
    "Salzach",
    "Usergroup",
    "Fira",
    "mailaddress",
    "wuauserv"
  ],
  "flagWords": [],
  "patterns": [
    {
      "name": "ALL-CAPS-WORDS",
      "pattern": "/\b[A-Z0-9]+\b/g",
      "description": "Any word in ALL CAPS."
    }
  ],
  "ignoreRegExpList": ["ALL-CAPS-WORDS", "Email", "github.com/", "@"]
}
